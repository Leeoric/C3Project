.main.container {
	padding-top: 25px;
}

.conf-header {
	position: relative;
	margin-bottom: 25px;
	padding: 20px;
	border: 1px solid #e4e4e4;
	background: #f6f7f8;
}

.conf-header h2 {
	margin-bottom: 38px;
}

.conf-header .td2 {
	vertical-align: bottom;
	font-size: 14px;
}

.conf-header .td3 {
	display: inline-block;
	margin-left: 20px;
	vertical-align: middle;
}

.conf-header .table-master {
	max-width: 600px;
	font-size: 14px;
}

.conf-header p {
	font-size: 14px;
	color: #5E5E5E;
}

.conf-header .handle {
	position: absolute;
	right: 10px;
	bottom: 10px;
}

.conf-header .handle span {
	width: 100px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 14px;
	color: #ffffff;
	cursor: pointer;
}

.conf-header .handle .signUp {
	/*position: absolute;*/
	/*right: 85px;*/
	/*bottom: 2px;*/
	width: 100px;
	background-color: #f00;
	border: 1px solid #ffffff;
}

.conf-header .handle span.signUped {
	background-color: #b6b6b6;
	border: 1px solid #ffffff;
}

.conf-header .handle .sign-up-num {
	/*position: absolute;*/
	/*right: 85px;*/
	/*bottom: 2px;*/
	color: #fff;

}

.conf-header .handle .started-meeting {
	width: 100px;
	background-color: #f00;
	border: 1px solid #ffffff;
}


.conf-header .handle .record {
	background-color: #e26332;
	display: inline-block;
	width: 100px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #fff;
	font-size: 14px;
	border: 1px solid #ffffff;
}

.conf-header .handle .sound {
	background-color: #eea002;
	display: inline-block;
	width: 100px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #fff;
	font-size: 14px;
	border: 1px solid #ffffff;
}

.conf-header .handle .fav {
	background-color: #3f8de9;
	/*background-color: #800080;*/
	border: 1px solid #ffffff;
}

.conf-header .handle .ed {
	background-color: #b6b6b6;
	border: 1px solid #ffffff;
	cursor: default;
}

.conf-header .handle .faved {
	background-color: #3f8de9;
	border: 1px solid #ffffff;
}

.conf-detail {
	height: 750px;
}

.conf-detail-content {
	width: 770px;
	/*margin-right: 20px;*/
}

.conf-view {
	width: 770px;
	/*height: 450px;*/
	/*position: relative;*/
	overflow: auto;
	border: 1px solid #ccc;
}

.conf-view .lunbo {
	/*width: 780px;*/
	height: 450px;
}

.conf-content-detail {
	margin: 0 0 20px 0;
	padding: 0 15px;
	border: 1px solid #e4e4e4;
	/*height: 733px;*/
}

.conf-content-detail h3 {
	padding-left: 12px;
	margin: 10px 0;
	border-left: 4px solid #5470b5;
	font-size: 16px;
}

.conf-intro {
	margin-bottom: 30px;
}

.conf-intro div {
	font-size: 14px;
	line-height: 24px;
	text-indent: 2em;
	color: #333;
}

.conf-intro td {
	padding: 10px 0;
	vertical-align: top;
}

.conf-intro.conf-attachment a {
	color: #5470b5;
}

.conf-intro.conf-attachment a:hover {
	color: #6ab7f5;
}

.interaction-box {
	position: relative;
	width: 315px;
	/*height: 550px;*/

	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	/*border: 1px solid #e4e4e4;*/
}

.interaction {
	height: 560px;
	overflow: auto;
	border: 1px solid #e4e4e4;
}

.interaction-box h3 {
	height: 25px;
	line-height: 25px;
	padding: 10px;
	background-color: #e4e4e4;
	border-bottom: 1px solid #e4e4e4;
	font-size: 16px;
}

.interaction-box .comment-number {
	color: #f00;
}

.dialog-box {
	line-height: 2;
	font-size: 14px;
	overflow-x: hidden;
	word-wrap: break-word;
}

.dialog {
	padding: 10px;
	border: 1px solid #e4e4e4;
	/*border-bottom: none;*/
}

.send-people-info span {
	font-size: 14px;
	cursor: pointer;
}

.send-people-info img {
	width: 18px;
}
.send-people-info .reply {
	color: #999;
}

.send-msg {
	position: relative;
	/*left: 0;*/
	/*bottom: -185px;*/
	width: 100%;
	padding: 10px 10px 0 10px;
	background-color: #e4e4e4;

	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.send-msg textarea {
	padding: .5em;
	resize: none;
	font-size: 14px;

	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;;
}

.send-msg .push-msg {
	*display: inline;
	display: inline-block;
	width: 50px;
	height: 25px;
	line-height: 23px;
	text-align: center;
	border: 1px solid #5470b5;
	color: #ffffff;
	font-size: 14px;
	cursor: pointer;

	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
	background-color: #5470b5;
}

