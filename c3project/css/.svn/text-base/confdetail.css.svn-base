.font-tip {
	display: inline-block;
	margin-right: 25px;
	line-height: 2.5;
	font-size: 12px;
	color: #a1a1a1;
}

.send-people-info span.comment-time {
	margin-left: 10px;
	font-size: 12px;
	color: #999;
}

/*浮出层B*/
/*float-div B*/
.float-div {
	position: fixed;
	left: 0;
	top: 0;
	border: 1px solid red;
	z-index: 1000;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,.5);
}

.float-info {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	z-index: 1000;
	width: 400px;
	height: 250px;
	border: 1px solid #a0a0a0;
	-webkit-border-radius: 15px;
	-moz-border-radius: 15px;
	border-radius: 15px;
	text-align: center;
	background-color: #fff;
}

.float-div-success .float-info {
	background-color: #53b2fe;
}

.float-div.float-div-success p {
	width: 210px;
	margin: 60px auto;
	color: #ffffff;
}

.float-info span {
	display: inline-block;
	*display: inline;
}

.float-info p {
	margin-top: 60px;
	padding: 20px 20px;
	/*text-align: left;*/
}

.float-cancel {
	position: absolute;
	right: 10px;
	top: 10px;
	cursor: pointer;
}

.float-confirm {
	/*margin: 10px auto;*/
	position: absolute;
	left: 150px;
	bottom: 20px;
	width: 100px;
	height: 35px;
	line-height: 35px;
	border: 1px solid #53b2fe;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	color: #ffffff;
	background-color: #53b2fe;
	cursor: pointer;
}

.float-div-success .float-confirm {
	background-color: #fff;
	color: #53b2fe;
}
